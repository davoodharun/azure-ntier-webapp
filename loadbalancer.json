{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "sqlLBName": {
      "type": "string"
    },
    "sqlLBFE": {
      "type": "string"
    },
    "sqlLBIPAddress": {
      "type": "string"
    },
    "sqlSubnetRef": {
      "type": "string"
    },
    "sqlLBBE": {
      "type": "string"
    },
    "sqlLBBEID": {
      "type": "string"
    },
    "sqllbFEConfigID": {
      "type": "string"
    },
    "sqllbProbeID": {
      "type": "string"
    },
    "SQLAOProbe": {
      "type": "string"
    },
    "sqlLoadBalancerTemplateURL": {
      "type": "string"
    }
  },
  "variables": {
  },
  "resources": [
    {
      "name": "SQLLoadBalancer",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "comments": "Create Load Balancers for SQL and DC",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[parameters('sqlLoadBalancerTemplateURL')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "location": {
            "value": "[resourceGroup().location]"
          },
          "sqlLBName": {
            "value": "[parameters('sqlLBName')]"
          },
          "sqlLBFE": {
            "value": "[parameters('sqlLBFE')]"
          },
          "sqlLBIPAddress": {
            "value": "[parameters('sqlLBIPAddress')]"
          },
          "staticSubnetRef": {
            "value": "[parameters('sqlSubnetRef')]"
          },
          "sqlLBBE": {
            "value": "[parameters('sqlLBBE')]"
          },
          "sqlLBBEID": {
            "value": "[parameters('sqlBEAddressPoolID')]"
          },
          "sqllbFEConfigID": {
            "value": "[parameters('sqllbFEConfigID')]"
          },
          "sqllbProbeID": {
            "value": "[parameters('sqllbProbeID')]"
          },
          "SQLAOProbe": {
            "value": "[parameters('SQLAOProbe')]"
          }
        }
      }
    },
  ]
}
